{
  "name": "spatial-planning",
  "version": "1.0.0",
  "private": true,
  "description": "空间规划系统",
  "license": "MIT",
  "author": "xavi",
  "main": "index.js",
  "workspaces": {
    "packages": [
      "packages/*",
      "backend",
      "client",
      "manage"
    ]
  },
  "scripts": {
    "build:backend": "pnpm -F @sp/backend build",
    "build:client": "pnpm -F @sp/client build",
    "build:manage": "pnpm -F @sp/manage build",
    "build:mock-client": "pnpm -F @sp/client build:mock",
    "build:mock-manage": "pnpm -F @sp/manage build:mock",
    "clean": "pnpm -r run clean && rimraf node_modules",
    "dev:backend": "pnpm -F @sp/backend dev",
    "dev:client": "pnpm -F @sp/client dev",
    "dev:manage": "pnpm -F @sp/manage dev",
    "dev:mock-backend": "pnpm -F @sp/backend dev:mock",
    "dev:mock-client": "pnpm -F @sp/client dev:mock",
    "lint": "pnpm -r run lint",
    "lint-staged": "lint-staged",
    "migrate": "pnpm -F @sp/backend migrate",
    "prepare": "husky install",
    "preview:client": "pnpm -F @sp/client preview",
    "preview:manage": "pnpm -F @sp/manage preview"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@alova/mock": "^1.5.1",
    "@alova/scene-vue": "^1.4.2",
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@codemirror/lang-html": "^6.4.7",
    "@codemirror/lang-json": "^6.0.1",
    "@sp/shared": "workspace:^",
    "@vueuse/core": "^10.6.1",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "alova": "^2.17.0",
    "ant-design-vue": "^4.1.1",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "mockjs": "^1.1.0",
    "pinia": "^2.1.7",
    "ramda": "^0.29.1",
    "spark-md5": "^3.0.2",
    "uuid": "^9.0.1",
    "vue": "^3.4.3",
    "vue-codemirror": "^6.1.1",
    "vue-draggable-plus": "^0.2.7",
    "vue-echarts": "^6.6.8",
    "vue-router": "^4.2.5",
    "vue3-colorpicker": "^2.2.3"
  },
  "devDependencies": {
    "@amap/amap-jsapi-types": "^0.0.13",
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@iconify/json": "^2.2.127",
    "@sp/vite-config": "workspace:^",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.8.4",
    "@types/ramda": "^0.29.10",
    "@types/spark-md5": "^3.0.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@unocss/eslint-config": "^0.56.5",
    "commitlint": "^17.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "unocss": "^0.56.5",
    "vite": "^4.5.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.27"
  },
  "packageManager": "pnpm@8.6.9",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "pnpm": {
    "patchedDependencies": {
      "@wangeditor/editor-for-vue@5.1.12": "patches/@wangeditor__editor-for-vue@5.1.12.patch",
      "@amap/amap-jsapi-types@0.0.13": "patches/@amap__amap-jsapi-types@0.0.13.patch"
    }
  }
}
